{
  "name": "deployment-monitoring-ops",
  "version": "1.0.0",
  "description": "Stack Prometheus + Grafana pour monitoring des services de d√©ploiement automatique",
  "type": "module",
  "main": "scripts/deployment-metrics-exporter.js",
  "scripts": {
    "start": "node scripts/deployment-metrics-exporter.js",
    "test": "node -c scripts/deployment-metrics-exporter.js",
    "docker:build": "docker build -f Dockerfile.metrics-exporter -t deployment-metrics-exporter:latest .",
    "docker:run": "docker run -p 9091:9091 deployment-metrics-exporter:latest",
    "stack:up": "docker compose up -d",
    "stack:down": "docker compose down",
    "stack:logs": "docker compose logs -f"
  },
